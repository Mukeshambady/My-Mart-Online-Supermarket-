<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">

  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta http-equiv="x-ua-compatible" content="ie=edge">
  <meta http-equiv="cache-control" content="max-age=0" />
  <meta http-equiv="cache-control" content="no-cache" />
  <title>My Mart Online Supermarket | {{title}}</title>
  <!-- Font Awesome -->
  <link rel="stylesheet" type="text/css" href="https://use.fontawesome.com/releases/v5.11.0/css/all.css">
  {{!--
  <link rel="stylesheet" type="text/css" href="../../fonts/all.css"> --}}

  <!-- Bootstrap core CSS -->
  {{!--
  <link rel="stylesheet" type="text/css"
    href="https://mdbootstrap.com/api/snippets/static/download/MDB-Pro_4.19.1/css/bootstrap.min.css"> --}}
  <link rel="stylesheet" type="text/css" href="../../css/bootstrap.min.css">
  <!-- Material Design Bootstrap -->
  {{!--
  <link rel="stylesheet" type="text/css"
    href="https://mdbootstrap.com/api/snippets/static/download/MDB-Pro_4.19.1/css/mdb.min.css"> --}}
  <link rel="stylesheet" type="text/css" href="../../css/mdb.min.css">
  <!-- JQuery -->
  {{!--
  <script src="https://mdbootstrap.com/api/snippets/static/download/MDB-Pro_4.19.1/js/jquery.min.js"></script> --}}
  <script src="../../js/jquery.min.js"></script>
  <script src="https://cdn.jsdelivr.net/jquery.validation/1.16.0/jquery.validate.min.js"></script>

  <!-- Your custom styles (optional) -->
  <style>
    .card.card-cascade .view.gradient-card-header {
      padding: 1.1rem 1rem;
    }

    .card.card-cascade .view {
      box-shadow: 0 5px 12px 0 rgba(0, 0, 0, 0.2), 0 2px 8px 0 rgba(0, 0, 0, 0.19);
    }

    /* Product Quantity */


    .quantity input {
      -webkit-appearance: none;
      border: none;
      text-align: center;
      width: 32px;
      font-size: 16px;
      color: #43484D;
      font-weight: 300;

    }

    .error,
    .validation-error-phonenumber {
      color: red;
    }


  </style>


</head>

<body class="fixed-sn white-skin">
  <!-- Main Navigation -->
  <header>
    {{#if session.user}}
    {{#if (eqIf session.user.state 0) }}
    {{#if (eqIf session.url '/admin') }}
    {{>_admin-side-bar}}
    {{/if}}
    {{/if}}

    {{#if (eqIf session.user.state 2)}}
    {{#if (eqIf session.url '/dealer') }}
    {{>_dealer-side-bar}}
    {{/if}}

    {{/if}}
    {{/if}}
    {{>_header-top}}


    <main class="mb-5 pb-5">
      {{{body}}}

      {{#if session.user}}
      {{#if (eqIf session.user.state 1)}}

      {{>_modals}}

      {{/if}}
      {{/if}}

    </main>
    <!--Main layout-->
    <div class="modal fade" id="view-tracks">
      <div class="modal-dialog modal-lg view-tracks">
        <div class="modal-content">
          <div class="modal-header">
            <h4 class="modal-title">Order Tracking Detailes</h4>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="modal-body">
          </div>
          {{!-- <div class="modal-footer justify-content-between">
            <button type="button" class="btn btn-info" data-dismiss="modal">Close</button>
          </div> --}}
        </div>
      </div>
    </div>
    {{!-- tracking --}}
    <div class="modal fade" id="show-tracks">
      <div class="modal-dialog modal-lg view-tracks">
        <div class="modal-content">
          <div class="modal-header">
            <h4 class="modal-title">Tracking Detailes</h4>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="modal-body">
          </div>
          {{!-- <div class="modal-footer justify-content-between">
            <button type="button" class="btn btn-info" data-dismiss="modal">Close</button>
          </div> --}}
        </div>
      </div>
    </div>


    <!-- Footer -->
    <footer class="page-footer fixed-bottom font-small mdb-color lighten-3">

      <!-- Copyright -->
      <div class="footer-copyright text-center py-3 ">&#xA9; 2020 Copyright:
        <a href="https://mdbootstrap.com/bootstrap-tutorial/"> MDBootstrap.com</a>
      </div>
      <!-- Copyright -->

    </footer>
    <!-- Footer -->

    <!-- SCRIPTS -->

    <!-- Bootstrap tooltips -->
    {{!--
    <script type="text/javascript"
      src="https://mdbootstrap.com/previews/templates/admin-dashboard/js/popper.min.js"></script> --}}
    <script type="text/javascript" src="../../js/popper.min.js"></script>
    <!-- Bootstrap core JavaScript -->
    {{!--
    <script src="https://mdbootstrap.com/api/snippets/static/download/MDB-Pro_4.19.1/js/bootstrap.min.js"></script> --}}
    <script src="../../js/bootstrap.min.js"></script>
    <!-- MDB core JavaScript -->
    {{!--
    <script type="text/javascript"
      src="https://mdbootstrap.com/previews/templates/admin-dashboard/js/mdb.min.js"></script> --}}
    <script type="text/javascript" src="../../js/mdb.min.js"></script>
    {{!-- jquery validate --}}
    <script src="https://cdn.jsdelivr.net/npm/jquery-validation@1.19.2/dist/jquery.validate.js"></script>

    <!-- Custom scripts -->

    <script>



      var container = document.querySelector('.custom-scrollbar');

      if (container) {
        // SideNav Initialization
        $(".button-collapse").sideNav();
        $("body").even().addClass("fixed-sn");
        Ps.initialize(container, {
          wheelSpeed: 2,
          wheelPropagation: true,
          minScrollbarLength: 20
        });
      } else {
        $("body").even().removeClass("fixed-sn");
      }
    </script>
    {{#if session.user}}
    {{#if (eqIf session.user.state 0)}}
    <script type="text/javascript" src="../../js/adminjs.js">   </script>
    {{/if}}

    {{#if (eqIf session.user.state 1)}}
    <script type="text/javascript" src="../../js/userjs.js">   </script>
    {{/if}}
    {{#if (eqIf session.user.state 2)}}
    <script type="text/javascript" src="../../js/dealerjs.js">   </script>
    {{/if}}
    {{else}}
    <script type="text/javascript" src="../../js/dealerjs.js">   </script>
    {{/if}}


    <script>
      $(function () {
        $('[data-toggle="tooltip"]').tooltip()
      })
    </script>

    <script>



      $('.orderStatus').on('click', async function (e) {
        await login_check()
        $('#view-tracks').modal().find('.modal-body').load('/get_ordered_status');
        $("#view-tracks").modal('show')
      });
      //track
      async function showTraking(id) {
        await login_check()
        $("#view-tracks").modal('hide')
        $('#show-tracks').modal().find('.modal-body').load('/get_ordered_track/' + id);
        $("#show-tracks").modal('show')
      };

 

    </script>



</body>

</html>